{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
    
  },
  "include": [
    "src/**/*.ts","src/components", "src/custom.d.ts"
, "src/pages", "src/contextAPI/AuthContext.tsx"  ],

  "exclude": [
    "typings/browser.d.ts",
    "typings/browser",
    "typings",
    "node_modules"
  ]

}
